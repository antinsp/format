- name: Socks5 (SNI)

  type: socks5

  server: SERVER.COM

  port: 443

  username: USERNAME

  password: PASSWORD

  tls: true

  skip-cert-verify: true

  udp: true

  sni: BUGSNI.COM

- name: Shadowsocks Tanpa Plugin

  type: ss

  server: SERVER.COM

  port: 34963

  cipher: chacha20-ietf-poly1305

  password: PASSWORD

  udp: true

- name: Shadowsocks Plugin Obfs

  type: ss

  server: SERVER.COM

  port: 32033

  cipher: chacha20-ietf-poly1305

  password: PASSWORD

  plugin: obfs

  plugin-opts:

    mode: tls

    host: BUGSNI.COM

- name: ShadowsocksR (SNI)

  server: SERVER.COM

  port: 1235

  type: ssr

  cipher: AES-256-CTR

  password: PASSWORD

  protocol: origin

  obfs: tls1.2_ticket_auth

  protocol-param: "#"

  obfs-param: BUGSNI.COM

  udp: true

- name: Snell V3

  type: snell

  server: SERVER.COM

  port: 33223

  psk: PASSWORD

  version: 3

  udp: true

  obfs-opts:

    mode: tls

    host: BUGSNI.COM

    

